    "target": "ES5",
    "module": "CommonJS",
    "lib": ["ES6"],
    "sourceMap": true,
    "strict": true,
    "outDir": "./build",
    "rootDir": "./src",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ]
}
